<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.security_demo2.mapper.UserMapper">

    <insert id="in" parameterType="com.example.security_demo2.entity.User">

    </insert>
    <select id="selectall" resultType="com.example.security_demo2.entity.User">
        select * from user
    </select>
    <select id="selectO" resultType="com.example.security_demo2.entity.User" parameterType="Integer">
        select * from user where id=#{hh};
    </select>
    <select id="selectOaa" resultType="com.example.security_demo2.entity.User" parameterType="com.example.security_demo2.entity.User">
        select * from user where id=#{id} and username=#{username};
    </select>
    <select id="" resultMap="">

    </select>
</mapper>
